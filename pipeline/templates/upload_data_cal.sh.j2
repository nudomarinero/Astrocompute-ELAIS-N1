#!/bin/bash
# Copy the important data to a S3 bucket
# Create bucket
s3cmd mb s3://{{ bucket_name }}
# Upload the logs
s3cmd put /mnt/scratch/data/log/profile.log s3://{{ bucket_name }}/{{ dataset_name }}/logs/{{ calibrator_name }}/
s3cmd put /mnt/scratch/data/log/pipeline* s3://{{ bucket_name }}/{{ dataset_name }}/logs/{{ calibrator_name }}/pipeline/
# Upload the data
s3cmd put /mnt/scratch/data/cal/{{ main_pipeline_name }}/*.h5 s3://{{ bucket_name }}/{{ dataset_name }}/{{ calibrator_name }}/h5/
s3cmd put /mnt/scratch/data/cal/{{ resume_pipeline_name }}/*.npy s3://{{ bucket_name }}/{{ dataset_name }}/{{ calibrator_name }}/npy/
# Upload the plots
s3cmd put /mnt/scratch/data/cal/{{ resume_pipeline_name }}/*.png s3://{{ bucket_name }}/{{ dataset_name }}/{{ calibrator_name }}/plots/png/
s3cmd put /mnt/scratch/data/cal/{{ resume_pipeline_name }}/*.pdf s3://{{ bucket_name }}/{{ dataset_name }}/{{ calibrator_name }}/plots/pdf/

